{
  "name": "boostid",
  "alias": "boostid",
  "version": 2,
  "builds": [
    { "src": "list-buckets.js", "use": "@now/node" },
    { "src": "ci.js", "use": "@now/node" },
    { "src": "**", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/list-buckets", "dest": "list-buckets.js" },
    { "src": "/circleci/(.*)", "dest": "ci.js" },
    { "src": "/[^\\.]*", "dest": "index.html" }
  ]
}